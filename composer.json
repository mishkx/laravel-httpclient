{
  "name": "mishkx/laravel-httpclient",
  "type": "library",
  "description": "Http Client",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Mikhail Pichushkin",
      "email": "pi4ushkin@gmail.com"
    }
  ],
  "require": {
